<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>criarEstabelecimento</servlet-name>
        <servlet-class>web.criarEstabelecimento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletarCerveja</servlet-name>
        <servlet-class>web.deletarCerveja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>web.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>web.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editarEstabelecimento</servlet-name>
        <servlet-class>web.editarEstabelecimento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>alterarSenha</servlet-name>
        <servlet-class>web.alterarSenha</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteEstabelecimento</servlet-name>
        <servlet-class>web.deleteEstabelecimento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getCervejaEstabelecimento</servlet-name>
        <servlet-class>web.getCervejaEstabelecimento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getCervejaBusca</servlet-name>
        <servlet-class>web.getCervejaBusca</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adicionarCerveja</servlet-name>
        <servlet-class>web.adicionarCerveja</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>criarEstabelecimento</servlet-name>
        <url-pattern>/criarEstabelecimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletarCerveja</servlet-name>
        <url-pattern>/deletarCerveja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editarEstabelecimento</servlet-name>
        <url-pattern>/editarEstabelecimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>alterarSenha</servlet-name>
        <url-pattern>/alterarSenha</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteEstabelecimento</servlet-name>
        <url-pattern>/deleteEstabelecimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getCervejaEstabelecimento</servlet-name>
        <url-pattern>/getCervejaEstabelecimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getCervejaBusca</servlet-name>
        <url-pattern>/getCervejaBusca</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adicionarCerveja</servlet-name>
        <url-pattern>/adicionarCerveja</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
